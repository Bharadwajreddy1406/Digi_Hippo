# Ignore node_modules in both frontend and backend
/frontend/node_modules/
/backend/node_modules/

# Ignore logs
/frontend/logs
/frontend/*.log
/frontend/logs/*.log
/backend/logs
/backend/*.log
/backend/logs/*.log

# Ignore build directories
/frontend/dist/
/frontend/build/
/backend/dist/
/backend/build/

/# Ignore dependency directories
/frontend/jspm_packages/
/frontend/bower_components/
/backend/jspm_packages/
/backend/bower_components/

/# Miscellaneous
/frontend/.DS_Store
/frontend/.env
/frontend/.vscode/
/frontend/.idea/
/frontend/*.sublime-project
/frontend/*.sublime-workspace
/backend/.DS_Store
/backend/.env
/backend/.vscode/
/backend/.idea/
/backend/*.sublime-project
/backend/*.sublime-workspace

# TypeScript
/frontend/*.tsbuildinfo
/backend/*.tsbuildinfo

# Optional npm cache directory
/frontend/.npm
/backend/.npm

# Optional eslint cache
/frontend/.eslintcache
/backend/.eslintcache

# Optional REPL history
/frontend/.node_repl_history
/backend/.node_repl_history

# Output of 'npm pack'
/frontend/*.tgz
/backend/*.tgz

# Yarn Integrity file
/frontend/.yarn-integrity
/backend/.yarn-integrity

# dotenv environment variables file
/frontend/.env.local
/frontend/.env.development.local
/frontend/.env.test.local
/frontend/.env.production.local
/backend/.env.local
/backend/.env.development.local
/backend/.env.test.local
/backend/.env.production.local

# Parcel-bundler cache (https://parceljs.org/)
/frontend/.cache
/frontend/.parcel-cache
/backend/.cache
/backend/.parcel-cache